        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
